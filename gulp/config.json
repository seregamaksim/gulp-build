{
  "root": {
    "source": "src",
    "build": "build"
  },
  "template": {
    "folders": "/template/**/*.pug",
    "dir": "/template/pages/*.*"
  },
  "style": {
    "folders": "/style/**/*.*",
    "dev": "/style/",
    "dest": "/css"
  },
  "js": {
    "folders": "/script/**/",
    "dist": "js/",
    "extension": "*.+(js|json)"
  },
  "images": {
    "dev": "/images/**/",
    "dist": "/images",
    "extension": "*.+(jpg|jpeg|png|gif|svg)"
  },
  "fonts": {
    "dev": "/fonts/**/",
    "dist": "/fonts",
    "extension": "*.+(woff|woff2|ttf|eot|svg)"
  },
  "sprite": {
    "dev": "/images/**/*.svg",
    "dist": "images/sprite"
  },
  "webp": {
    "dev": "/images/**/",
    "dist": "/images",
    "extension": "*.+(jpg|jpeg|png|gif)"
  },
  "favicons": {
    "dev": "/favicons/**/*.*",
    "dest": "/"
  }
}
  